% overrides
% make default \geq and \leq be slanted \geqslant \leqslant
\let\geqflat\geq
\let\geq\geqslant
\let\leqflat\leq
\let\leq\leqslant
% i hate the standard \epsilon and prefer \varepsilon, so swap them
\let\tempepsiloncopy\epsilon
\let\epsilon\varepsilon
\let\varepsilon\tempepsiloncopy
% make colour of cancellation (\cancel, \bcancel, \cancelto) blue for clarity
\renewcommand\CancelColor{\color{blue}}

% typesetting
